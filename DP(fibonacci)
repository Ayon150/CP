#include<bits/stdc++.h>
using namespace std;
#define ll long long int
#define fast ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL)
#define endl "\n"

int fibo(int n,vector<int>& mem){
    mem[0]=0;
    mem[1]=1;

    if(mem[n] != -1){
        return mem[n];
    }

    mem[n] = fibo(n-1,mem)+fibo(n-2,mem);

    return mem[n];

}

int fibonacci(int n){
    vector<int>mem(n+1,-1);
    return fibo(n,mem);
}

int main(){
    int n;
    cin>>n;
    cout<<fibonacci(n)<<endl;
}
